SET TALK OFF
USE NEW.DBF IN 1
USE NEW1.DBF IN 2
GO TOP IN 1
GO TOP IN 2
SELECT NEW
DO WHILE .NOT. EOF(2)
  APPEND BLANK
  REPLACE NEW->INGLES WITH NEW1->INGLES
  REPLACE NEW->ESPANOL WITH NEW1->ESPANOL
  REPLACE NEW->NIVING WITH NEW1->NIVEL
  REPLACE NEW->NIVESP WITH NEW1->NIVEL
  REPLACE NEW->DATEING WITH SUBSTR(DTOS(DATE()),3,6)
  REPLACE NEW->DATEESP WITH SUBSTR(DTOS(DATE()),3,6)
  SKIP 1 IN 2
ENDDO
CLOSE DATABASES
